<aura:component
implements = "lightning:appHomeTemplate"
access="global"
description="Theree colums description">

<aura:attribute name="left" type="Aura.Component[]" access="global" />
<aura:attribute name="center" type="Aura.Component[]" access="global" />
<aura:attribute name="right" type="Aura.Component[]" access="global" />
<div aura:id="container">
    <lightning:layout>
        <lightning:layoutItem size="2" aura:id="leftColumn">
            {!v.left}
        </lightning:layoutItem>
        <lightning:layoutItem size="7" aura:id="centerColumn" class="center">
            {!v.center}
        </lightning:layoutItem>
        <lightning:layoutItem size="3" aura:id="rightColumn">
            {!v.right}
        </lightning:layoutItem>
    </lightning:layout>
</div>


</aura:component>